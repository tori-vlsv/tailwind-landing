{
  "name": "rise",
  "version": "1.0.0a",
  "description": "rise",
  "author": {
    "name": "wotkad",
    "email": "wotkad@gmail.com",
    "url": "https://wotkad.ru"
  },
  "main": "src/bundle.js",
  "repository": "git@github.com:wotkad/rise.git",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && webpack --env mode=production --config ./webpack.config.js --progress && node ./src/scripts/changeSitemapUrls && node ./src/scripts/replaceErrorPageDist && rimraf ./dist/404",
    "develop": "webpack serve --env mode=development --config ./webpack.config.js --progress",
    "typecheck": "tsc --p .",
    "clear": "rimraf ./src/markdown/build && rimraf ./src/views/pages/blog/",
    "compile": "cross-env-shell babel src/markdown/constructor -d ./src/markdown/build --extensions '.mts'",
    "start": "yarn clear && yarn compile && node ./src/scripts/renameJsFiles && node ./src/markdown/build/index.mjs",
    "start-typecheck": "yarn typecheck && yarn start",
    "dev-network": "webpack serve --env mode=development --config ./webpack.config.js --progress --open --host 0.0.0.0 --disable-host-check",
    "dev": "yarn start-typecheck && rimraf ./src/markdown/build/decs.d.mjs && yarn develop"
  },
  "resolutions": {
    "upath": "1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^7.27.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.1",
    "highlight.js": "^11.5.0",
    "html-webpack-plugin": "^5.3.1",
    "html2pug": "^4.0.0",
    "http-server": "^14.1.0",
    "markdown-it": "^12.3.2",
    "markdown-it-meta": "^0.0.1",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.3.0",
    "postcss-modules": "^4.0.0",
    "postcss-preset-env": "^6.7.0",
    "pug": "^3.0.2",
